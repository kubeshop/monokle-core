{
  "name": "@monokle/parser",
  "version": "0.3.2",
  "description": "Kubernetes resource parser",
  "author": "Kubeshop",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "lib/index.js",
  "source": "src/index.ts",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "node": "./lib/index.js",
      "module": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib/**"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:snapshot": "echo \"No tests\"",
    "build": "rimraf lib && tsc --build tsconfig.build.json",
    "format:all": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubeshop/monokle-core.git"
  },
  "keywords": [
    "monokle",
    "kubernetes",
    "parser",
    "yaml"
  ],
  "bugs": {
    "url": "https://github.com/kubeshop/monokle-core/issues"
  },
  "homepage": "https://github.com/kubeshop/monokle-core/tree/main/packages/parser",
  "devDependencies": {
    "@types/lodash": "4.14.196",
    "rimraf": "3.0.2",
    "typescript": "4.8.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "path-browserify": "^1.0.1",
    "yaml": "2.2.2"
  }
}
