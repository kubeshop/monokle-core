{
    "name": "ui",
    "version": "0.0.0",
    "author": "Kubeshop",
    "description": "UI library for Monokle",
    "repository": {
        "type": "git",
        "url": "git://github.com/kubeshop/monokle-core.git"
      },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "antd": "4.22.4",
        "@rjsf/antd": "4.2.0",
        "@ant-design/icons": "4.7.0",
        "styled-components": "5.3.5"
    },
    "devDependencies": {
        "react-use": "^17.4.0",
        "@babel/core": "7.17.8",
        "@storybook/addon-a11y": "6.5.0-alpha.50",
        "@storybook/addon-actions": "6.5.0-alpha.50",
        "@storybook/addon-docs": "6.5.0-alpha.50",
        "@storybook/addon-essentials": "6.5.0-alpha.50",
        "@storybook/addon-links": "6.5.0-alpha.50",
        "@storybook/react": "6.5.0-alpha.49",
        "@types/node": "17.0.23",
        "@types/react": "18.0.14",
        "@types/react-dom": "18.0.5",
        "@types/styled-components": "5.1.25",
        "@vitejs/plugin-react": "2.1.0",
        "acorn-jsx": "5.3.2",
        "babel-loader": "8.2.4",
        "json": "11.0.0",
        "storybook-builder-vite": "0.1.21",
        "typescript": "4.8.3",
        "vite": "3.1.0"
    },
    "license": "MIT",
    "peerDependencies": {
        "react": "16.8.0 || 18.x",
        "react-dom": "16.8.0 || 18.x",
        "styled-components": "5.0.0"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/core.umd.js",
    "module": "./dist/core.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/core.es.js",
            "require": "./dist/core.umd.js"
        }
    }
}
